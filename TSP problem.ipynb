   "source": [
    "import tsp\n",
    "matrix = [[0,100,300,100,75],\n",
    "          [100,0,50,75,125],\n",
    "          [300,50,0,100,125],\n",
    "          [100,75,100,0,50],\n",
    "          [75,125,125,50,0]]\n",
    "r = range(len(matrix))\n",
    "\n",
    "shortestpath = {(i,j):matrix[i][j] for i in r for j in r}\n",
    "print(tsp.tsp(r,shortestpath))"
   ]
